  "author": "Michael Uloth <hello@michaeluloth.com>",
  "private": true,
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ie_mob 11"
  ],
  "dependencies": {
    "@mdx-js/mdx": "^1.6.21",
    "@mdx-js/react": "^1.6.21",
    "@svgr/webpack": "^5.5.0",
    "@xstate/react": "^1.0.3",
    "dotenv": "^8.2.0",
    "gatsby": "^2.26.1",
    "gatsby-cli": "^2.13.1",
    "gatsby-image": "^2.5.0",
    "gatsby-plugin-catch-links": "^2.4.0",
    "gatsby-plugin-feed": "^2.7.0",
    "gatsby-plugin-google-analytics": "^2.5.0",
    "gatsby-plugin-manifest": "^2.6.0",
    "gatsby-plugin-mdx": "^1.4.0",
    "gatsby-plugin-netlify": "^2.5.0",
    "gatsby-plugin-postcss": "^3.1.0",
    "gatsby-plugin-react-helmet": "^3.4.0",
    "gatsby-plugin-remote-images": "1.0.7",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-robots-txt": "^1.5.3",
    "gatsby-plugin-sharp": "^2.8.0",
    "gatsby-plugin-sitemap": "^2.6.0",
    "gatsby-plugin-svgr": "^2.0.2",
    "gatsby-plugin-twitter": "^2.4.0",
    "gatsby-remark-a11y-emoji": "^1.1.0",
    "gatsby-remark-embedder": "^4.1.0",
    "gatsby-remark-images": "^3.5.1",
    "gatsby-remark-smartypants": "^2.4.0",
    "gatsby-remark-unwrap-images": "^1.0.2",
    "gatsby-remark-vscode": "^3.2.0",
    "gatsby-source-filesystem": "^2.5.0",
    "gatsby-transformer-remark": "^2.10.0",
    "gatsby-transformer-sharp": "^2.6.0",
    "gatsby-transformer-yaml": "^2.5.0",
    "get-youtube-id": "^1.0.1",
    "postcss-import": "12.0.1",
    "postcss-preset-env": "^6.7.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-is": "^17.0.1",
    "shortid": "^2.2.16",
    "string-replace-to-array": "^1.0.3",
    "tailwindcss": "^1.9.6",
    "theme-dracula": "github:dracula/visual-studio-code",
    "what-input": "^5.2.10",
    "xstate": "^4.14.0"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/mdx-js__react": "^1.5.3",
    "@types/node": "^14.14.7",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-helmet": "^6.1.0",
    "@types/shortid": "0.0.29",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-gatsby": "^0.6.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "stylelint-config-recommended": "^3.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "scripts": {
    "build:theme-dracula": "(cd node_modules/theme-dracula && npm i && npm run build)",
    "build": "npm run build:theme-dracula && gatsby build",
    "build:local": "npm run validate && npm run clean && npm run build",
    "check-format": "npm run prettier -- --list-different",
    "check-types": "tsc",
    "clean": "gatsby clean",
    "dev": "rm -rf .cache && GATSBY_HOT_LOADER=fast-refresh gatsby develop",
    "format": "npm run prettier -- --write",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}' --ignore-path .gitignore",
    "prettier": "prettier \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|css|html|md|mdx|graphql)\" --ignore-path .gitignore",
    "serve": "npm run build:local && gatsby serve",
    "test": "jest",
    "test:watch": "jest --watch",
    "validate": "npm-run-all --parallel check-types check-format lint"
  }
}
